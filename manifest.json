{
  "manifest_version": 3,
  "name": "CV_lib",
  "version": "1.0",
  "permissions": [
    "storage", "declarativeContent", "activeTab", "scripting","tabs"
  ],
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
  
    {
      "matches": ["<all_urls>"],
      "js": ["popup1.js"],
      "run_at": "document_end"
    }
  ]
}
